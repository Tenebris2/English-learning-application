<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane fx:id="mainPane" prefHeight="720.0" prefWidth="1080.0" style="-fx-background-color: radial-gradient(focus-angle 0deg, focus-distance 0%, center 50% 0%, radius 220%, #131f24 10%, #302b63 40%, #24243e 65%);" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.englishlearningappv1.Controllers.HelloResultController">
    <children>
        <Label fx:id="marks" alignment="CENTER" layoutX="250" layoutY="176.0" prefHeight="130.0" prefWidth="580.0" style="-fx-background-color: white; -fx-background-radius: 16px; -fx-border-radius: 16px;" textAlignment="CENTER" textFill="#203bd7" wrapText="true">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
            <padding>
                <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
            </padding>
        </Label>
        <Button id="mainMenu" fx:id="mainMenu" layoutX="407.0" layoutY="388.0" mnemonicParsing="false" onAction="#mainMenuClicked" prefHeight="68.0" prefWidth="265.0" style="-fx-background-color: dodgerblue; -fx-background-radius: 34px; -fx-border-radius: 34px; -fx-cursor: hand;" text="Back" textFill="WHITE">
            <font>
                <Font name="System Bold" size="27.0" />
            </font>
        </Button>
        <ProgressIndicator fx:id="correct_progress" layoutX="250.0" layoutY="176.0" minHeight="130.0" minWidth="130.0" prefHeight="232.0" prefWidth="258.0" progress="0.43" style="-fx-background-color: white; -fx-progress-color: green; -fx-font-weight: 900; -fx-font-size: 18px; -fx-background-radius: 16px; -fx-border-radius: 16px; -fx-box-border: green;">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </ProgressIndicator>
        <Label layoutX="380.0" layoutY="239.0" prefHeight="66.0" prefWidth="438.0" text="Play again and let the same group improve their score or see if new players can beat this result." wrapText="true">
            <font>
                <Font name="System Bold" size="17.0" />
            </font></Label>
        <Label layoutX="380.0" layoutY="176.0" prefHeight="29.0" prefWidth="450.0" text="Well Played!" wrapText="true">
            <font>
                <Font name="System Bold" size="44.0" />
            </font>
        </Label>
    </children>
    <ImageView fx:id="close" fitHeight="99.0" fitWidth="21.0" layoutX="1044.0" layoutY="14.0" onMouseClicked="#closeFunction" onMouseEntered="#inEffects3" onMouseExited="#outEffects3" style="-fx-cursor: hand;" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@image/icons8-close-50.png" />
        </image>
    </ImageView>
    <ImageView fx:id="minimize" fitHeight="99.0" fitWidth="21.0" layoutX="1012.0" layoutY="14.0" onMouseClicked="#minimizeFunction" onMouseEntered="#inEffects3" onMouseExited="#outEffects3" style="-fx-cursor: hand;" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@image/icons8-minimize-50.png" />
        </image>
    </ImageView>
</AnchorPane>
